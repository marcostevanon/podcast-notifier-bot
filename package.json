{
  "name": "spotify-podcast-notifier-bot",
  "version": "1.0.1",
  "description": "Get notifications when your favorite podcasts released new episodes on Spotify",
  "main": "index.js",
  "scripts": {
    "postinstall": "netlify-lambda install",
    "start:func": "netlify-lambda serve src/functions",
    "build:func": "netlify-lambda build src/functions",
    "botdev": "nodemon --ext ts --watch src --exec './node_modules/.bin/ts-node' src/index.ts",
    "start": "npm run build && node build/index.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/marcostevanon/podcast-notifier-bot.git"
  },
  "author": "marco.stevanon@gmail.com",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/marcostevanon/podcast-notifier-bot/issues"
  },
  "homepage": "https://github.com/marcostevanon/podcast-notifier-bot#readme",
  "type": "module",
  "devDependencies": {
    "@babel/preset-env": "^7.12.13",
    "@babel/preset-typescript": "^7.12.13",
    "@types/aws-lambda": "^8.10.71",
    "@types/express": "^4.17.11",
    "@types/lodash": "^4.14.168",
    "@types/mongodb": "^3.6.3",
    "@types/morgan": "^1.9.2",
    "@types/node": "^14.14.22",
    "@types/node-cron": "^2.0.3",
    "morgan": "^1.10.0",
    "ngrok": "^3.4.0",
    "nodemon": "^2.0.7",
    "ts-node": "^9.1.1",
    "tslint": "^6.1.3",
    "typescript": "^4.1.3"
  },
  "dependencies": {
    "aws-lambda": "^1.0.6",
    "axios": "^0.21.1",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "lodash": "^4.17.20",
    "mongodb": "^3.6.3",
    "netlify-lambda": "^2.0.3",
    "node-cron": "^2.0.3",
    "qs": "^6.9.6",
    "telegraf": "^4.0.2"
  }
}
